!keycode 151 =
keycode 37  = Control_L
keycode 133 = Meta_L Alt_L
keycode 64  = Super_L
keycode 65  = space
keycode 108 = Super_R
!keycode 117 = Menu
keycode 135 = Mode_switch
keycode 105 = Control_R

! arrows (normal)
keycode 166 = XF86ApplicationLeft
keycode 111 = Up Up Up Up
keycode 167 = XF86ApplicationRight
keycode 113 = Left Left Left
keycode 116 = Down Down Down Down
keycode 114 = Right Right Right

! arrows (Fn)
keycode 174 = XF86AudioStop
keycode 173 = XF86AudioPrev
keycode 172 = XF86AudioPlay
keycode 171 = XF86AudioNext

! special buttons (Logitech keyboard)
keycode 180 = XF86WWW
keycode 163 = XF86Mail
keycode 225 = XF86Search

! Original key layout:
! 
! shift       Shift_L (0x32),  Shift_R (0x3e)
! lock        Caps_Lock (0x42)
! control     Control_L (0x25),  Control_R (0x6d)
! mod1        Alt_L (0x40),  Alt_R (0x71),  Meta_L (0x9c)
! mod2        Num_Lock (0x4d)
! mod3      
! mod4        Super_L (0x7f),  Hyper_L (0x80)
! mod5        Mode_switch (0x5d),  ISO_Level3_Shift (0x7c)

clear shift
clear lock
clear control
clear mod1
clear mod2
clear mod3
clear mod4
clear mod5

add shift   = Shift_L Shift_R
add control = Control_L Control_R Caps_Lock
add mod1    = Alt_L Alt_R
add mod2    = Num_Lock
add mod4    = Super_L Super_R
add mod5    = Mode_switch ISO_Level3_Shift
